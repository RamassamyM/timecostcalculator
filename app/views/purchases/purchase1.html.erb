<div class="banner banner_type_small">
  <div class="container">
    <div class="banner__content">
      <h1 class="banner__title banner__title_type_smallbanner">FCA purchase</h1>
    </div>
  </div>
</div>
<div class="breadcrumb">
  <%= link_to calculations_path, class: '' do %>
    <i class="fas fa-chevron-left"></i> Back
  <% end %>
</div>
<div class="purchase-scheme-picture">
  <%= image_tag 'FCA_purchase.png' %>
</div>
<h3>Suppliers and places of loading</h3>
<% @suppliers_and_places_of_loading.each do |place| %>
  <p>
    <%= place %>
  </p>
<% end %>
<h3>All places of delivery</h3>
<% @all_places_of_delivery.each do |place| %>
  <p>
    <%= place %>
  </p>
<% end %>